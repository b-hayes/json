{
    "name": "test me",
    "description": "yo yo",
    "version": "1.0.0-alpha3",
    "type": "package",
    "keywords": [
        "php"
    ],
    "homepage": "https://getcomposer.org/doc/04-schema.md#homepage",
    "time": "yy-mm-dd",
    "license": [
        "LGPL-2.1-only",
        "GPL-3.0-or-later"
    ],
    "authors": [
        {
            "name": "Nils Adermann",
            "email": "naderman@naderman.de",
            "homepage": "https://www.naderman.de",
            "role": "Developer"
        },
        {
            "name": "Jordi Boggiano",
            "email": "j.boggiano@seld.be",
            "homepage": "https://seld.be",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@example.org",
        "irc": "irc://irc.freenode.org/composer"
    },
    "funding": [
        {
            "type": "patreon",
            "url": "https://www.patreon.com/phpdoctrine"
        },
        {
            "type": "tidelift",
            "url": "https://tidelift.com/subscription/pkg/packagist-doctrine_doctrine-bundle"
        },
        {
            "type": "other",
            "url": "https://www.doctrine-project.org/sponsorship.html"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-mbstring": "*"
    },
    "conflict": {
        "php": ">=7.4",
        "ext-mbstring": "*"
    },
    "replace": {
        "php": ">=7.4",
        "ext-mbstring": "*"
    },
    "provide": {
        "php": ">=7.4",
        "ext-mbstring": "*"
    },
    "suggest": {
        "monolog/monolog": "Allows more advanced logging of the application flow",
        "ext-xml": "Needed to support XML format in class Foo"
    },
    "autoload": {
        "psr-4": {
            "MyLibrary\\": "src/"
        },
        "psr-0": {
            "": "src/"
        },
        "classmap": [
            "src/addons/*/lib/",
            "3rd-party/*",
            "Something.php"
        ],
        "files": [
            "src/MyLibrary/functions.php"
        ],
        "exclude-from-classmap": [
            "/Tests/",
            "/test/",
            "/tests/"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.example.com"
        },
        {
            "type": "composer",
            "url": "https://packages.example.com",
            "options": {
                "ssl": {
                    "verify_peer": "true"
                }
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/Seldaek/monolog"
        },
        {
            "type": "package",
            "package": {
                "name": "smarty/smarty",
                "version": "3.1.7",
                "dist": {
                    "url": "https://www.smarty.net/files/Smarty-3.1.7.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://smarty-php.googlecode.com/svn/",
                    "type": "svn",
                    "reference": "tags/Smarty_3_1_7/distribution/"
                }
            }
        }
    ],
    "scripts": {
        "test": "some-test"
    },
    "extra": null,
    "bin": [
        "bin/my-script",
        "bin/my-other-script"
    ],
    "archive": {
        "name": "Strange_name"
    },
    "abandoned": true,
    "require-dev": {
        "php": ">=7.4",
        "ext-mbstring": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "MyLibrary\\Tests\\": "tests/"
        }
    },
    "include-path": [
        "lib/"
    ],
    "target-dir": "Symfony/Component/Yaml",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "non-feature-branches": [
        "latest-.*"
    ]
}